{
  "name": "keyterms-api",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app/app.js",
    "test": "NODE_ENV=TEST mocha -R spec --timeout 5000",
    "single-test": "NODE_ENV=TEST mocha -R spec  ./test/$FILE --timeout 5000 || :",
    "single-test-example": "FILE=\"01-server.js\" npm run single-test || :",
    "mocha": "NODE_ENV=TEST mocha -R spec $FILE || :",
    "lint": "eslint --ignore-pattern '**/public/**' app/**",
    "gen-docs": "apidoc -i ./app -o ./docs/dist -t ./docs/template > /dev/null",
    "init-cli": "node ./app/bin/cli/init",
    "admin-cli": "node ./app/bin/cli/admin",
    "glossary-cli": "node ./app/bin/cli/glossary",
    "reindex": "./app/bin/cli/reindex",
    "clean-tags": "./app/bin/cli/cleantags"
  },
  "dependencies": {
    "bcrypt": "~0.8.7",
    "bluebird": "^3.4.7",
    "body-parser": "~1.15.1",
    "connect-flash": "~0.1.1",
    "connect-mongodb-session": "^1.3.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cors-gate": "^1.0.0",
    "ejs": "~2.4.1",
    "elasticsearch": "^12.1.3",
    "escape-html": "^1.0.3",
    "exceljs": "0.2.37",
    "express": "~4.13.4",
    "express-fileupload": "0.0.5",
    "express-session": "^1.14.0",
    "extend": "^3.0.0",
    "fs": "0.0.1-security",
    "git-rev-sync": "^1.11.1",
    "jsonfile": "^4.0.0",
    "mongodb": "^2.2.23",
    "mongoose": "^4.5.10",
    "morgan": "^1.7.0",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "path": "^0.12.7",
    "promise-queue": "^2.2.3",
    "request": "^2.75.0",
    "serve-favicon": "~2.3.0",
    "superagent-defaults": "^0.1.14",
    "uuid": "^3.1.0",
    "winston": "~2.2.0",
    "winston-daily-rotate-file": "^1.3.1",
    "xml2js": "0.4.17"
