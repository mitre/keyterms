  "apidoc": {
    "title": "KeyTerms API v3"
  },
  "devDependencies": {
    "apidoc": "~0.16.1",
    "eslint": "https://registry.npmjs.org/eslint/-/eslint-4.19.1.tgz",
    "eslint-config-secure": "^0.2.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  }
